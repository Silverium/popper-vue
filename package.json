{
  "name": "@soldeplata/popper-vue",
  "version": "1.0.1",
  "description": "Popperjs implemented in a vue single file component",
  "main": "dist/popper-vue.ssr.js",
  "browser": "dist/popper-vue.esm.js",
  "module": "dist/popper-vue.esm.js",
  "unpkg": "dist/popper-vue.min.js",
  "types": "popper-vue.d.ts",
  "files": [
    "dist/*",
    "popper-vue.d.ts",
    "src/**/*.vue",
    "!src/serve-dev.*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/serve-dev.ts",
    "build-gh-pages": "vue-cli-service build src/serve-dev.ts --inline-vue --dest docs ",
    "deploy-gh-pages": "git checkout -B gh-pages && npm run build-gh-pages && git add dist -f && git commit dist -m 'gh-pages' --no-verify && git subtree push --prefix dist origin gh-pages && git checkout master && git branch -D gh-pages",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "@popperjs/core": "^2.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-replace": "^2.3.1",
    "@types/lodash": "^4.14.149",
    "@vue/cli-plugin-babel": "^4.2.2",
    "@vue/cli-plugin-typescript": "^4.2.2",
    "@vue/cli-service": "^4.2.2",
    "cross-env": "^6.0.3",
    "lodash": "4.17.15",
    "minimist": "^1.2.0",
    "node-sass": "^4.13.1",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-vue": "^5.1.6",
    "sass-loader": "^8.0.2",
    "typescript": "^3.8.2",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Silverium/popper-vue.git"
  },
  "keywords": [
    "vue",
    "popper",
    "popperjs",
    "popup",
    "tooltip",
    "vuejs"
  ],
  "author": "soldeplatadeveloper@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Silverium/popper-vue/issues"
  },
  "homepage": "https://github.com/Silverium/popper-vue#readme"
}
