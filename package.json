{
  "name": "VuePopper",
  "version": "1.0.0",
  "description": "",
  "main": "dist/vue-popper.ssr.js",
  "browser": "dist/vue-popper.esm.js",
  "module": "dist/vue-popper.esm.js",
  "unpkg": "dist/vue-popper.min.js",
  "types": "vue-popper.d.ts",
  "files": [
    "dist/*",
    "vue-popper.d.ts",
    "src/**/*.vue",
    "!src/serve-dev.*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve src/serve-dev.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {
    "@popperjs/core": "^2.0.6"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-replace": "^2.2.1",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-typescript": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "cross-env": "^6.0.3",
    "minimist": "^1.2.0",
    "node-sass": "^4.13.1",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "^5.1.5",
    "sass-loader": "^8.0.2",
    "typescript": "^3.7.3",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "engines": {
    "node": ">=8"
  }
}
